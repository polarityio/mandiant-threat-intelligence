{{#if details.errorMessage}}
  <div class="mt-2">
    <div class="alert alert-danger">
      <pre>{{details.errorMessage}}</pre>
    </div>
    {{#if (not (eq details.allowRetry false))}}
      <button
        type="button"
        class="p-btn btn btn-sm btn-secondary retry-btn mt-2"
        {{action "retryLookup"}}
      >
        {{#if running}}
          {{fa-icon icon="spinner-third" fixedWidth=true spin=true}}
        {{else}}
          Retry Search
        {{/if}}
      </button>
      <div class="mt-1">
        <span class="p-footnote">
          You can retry your search by pressing the "Retry Search" button.</span>
      </div>
    {{/if}}
  </div>
{{else}}
  {{#if details}}
    <div class="details">
      <ul class="mb-2 nav nav-tabs">
        {{#if details.indicatorV4}}
          <li class="nav-item">
            <a
              {{action "changeTab" "indicatorV4"}}
              class="nav-link {{if (eq activeTab 'indicatorV4') 'active'}}"
            >
              {{get displayTabNames "indicatorV4"}}
            </a>
          </li>
        {{/if}}
        {{#if details.vulnerabilities.length}}
          <li class="nav-item">
            <a
              {{action "changeTab" "vulnerabilities"}}
              class="nav-link {{if (eq activeTab 'vulnerabilities') 'active'}}"
            >
              {{get displayTabNames "vulnerabilities"}}
            </a>
          </li>
        {{/if}}
        {{#if details.threatActorResults}}
          <li class="nav-item">
            <a
              {{action "changeTab" "threatActorResults"}}
              class="nav-link {{if (eq activeTab 'threatActorResults') 'active'}}"
            >
              {{get displayTabNames "threatActorResults"}}
            </a>
          </li>
        {{/if}}
        {{#if details.searchResults.length}}
          <li class="nav-item">
            <a
              {{action "changeTab" "searchResults"}}
              class="nav-link {{if (eq activeTab 'searchResults') 'active'}}"
            >
              {{get displayTabNames "searchResults"}}
            </a>
          </li>
        {{/if}}
      </ul>
    </div>

    {{#if (eq activeTab "indicatorV4")}}
      <div class="indicator-v4-results">
        {{#with details.indicatorV4 as |indicatorV4|}}
          <div class="p-link">
            <a
              href="https://advantage.mandiant.com/indicator/{{indicatorV4.type}}/{{indicatorV4.value}}"
            >
              View in Mandiant
              {{fa-icon icon="external-link-square" class="external-link-icon"}}
            </a>
          </div>

          {{#if indicatorV4.attributed_associations}}
            <div class="attributed-associations">
              {{#if (get expandableTitleStates (concat "attributedAssociations" 0))}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action "toggleExpandableTitle" "attributedAssociations" 0)
                  }}
                >
                  <span class="text">Attributed Associations</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 attributed-associations-content">
                  {{#each
                    indicatorV4.attributed_associations
                    as |attributed_association|
                  }}
                    <div class="attributed-association"></div>
                    <div>
                      <span class="p-key">Name:</span>
                      <span class="p-value">{{attributed_association.name}}</span>
                    </div>
                    <div>
                      <span class="p-key">Type:</span>
                      <span class="p-value">{{attributed_association.type}}</span>
                    </div>
                  {{/each}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action "toggleExpandableTitle" "attributedAssociations" 0)
                  }}
                >
                  <span class="text">Attributed Associations</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            </div>
          {{/if}}

          {{#if (or indicatorV4.mscore (eq indicatorV4.mscore 0))}}
            <div>
              <span class="p-key">ThreatScore:</span>
              <span class="p-value">{{indicatorV4.mscore}}</span>
            </div>
          {{/if}}
          {{#if indicatorV4.type}}
            <div>
              <span class="p-key">Type:</span>
              <span class="p-value">{{indicatorV4.type}}</span>
            </div>
          {{/if}}
          {{#if indicatorV4.value}}
            <div>
              <span class="p-key">Value:</span>
              <span class="p-value">{{indicatorV4.value}}</span>
            </div>
          {{/if}}
          <div>
            <span class="p-key">Is Publishable:</span>
            <span class="p-value">{{or indicatorV4.is_publishable "false"}}</span>
          </div>
          <div>
            <span class="p-key">Is Exclusive to Threat Actor:</span>
            <span class="p-value">{{or indicatorV4.is_exclusive "false"}}</span>
          </div>
          {{#if indicatorV4.first_seen}}
            <div>
              <span class="p-key">First Seen:</span>
              <span class="p-value">{{moment-format
                  indicatorV4.first_seen
                  "MM/DD/YYYY HH:mm:ss z"
                  timeZone=timezone
                }}</span>
            </div>
          {{/if}}
          {{#if indicatorV4.last_updated}}
            <div>
              <span class="p-key">Last Updated:</span>
              <span class="p-value">{{moment-format
                  indicatorV4.last_updated
                  "MM/DD/YYYY HH:mm:ss z"
                  timeZone=timezone
                }}</span>
            </div>
          {{/if}}
          {{#if indicatorV4.last_seen}}
            <div>
              <span class="p-key">Last Seen:</span>
              <span class="p-value">{{moment-format
                  indicatorV4.last_seen
                  "MM/DD/YYYY HH:mm:ss z"
                  timeZone=timezone
                }}</span>
            </div>
          {{/if}}

          {{#if indicatorV4.reports}}
            <div class="indicator-reports">
              {{#if (get expandableTitleStates (concat "indicatorReports" 0))}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "indicatorReports" 0)}}
                >
                  <span class="text">Reports</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 indicator-reports-content">
                  {{#each indicatorV4.reports as |report|}}
                    <div class="mb-2 indicator-report">
                      {{#if report.title}}
                        <div>
                          <span class="p-key">Title:</span>
                          <span class="p-value">{{report.title}}</span>
                        </div>
                      {{/if}}
                      {{#if report.published_date}}
                        <div>
                          <span class="p-key">Published Date:</span>
                          <span class="p-value">{{moment-format
                              report.published_date
                              "MM/DD/YYYY HH:mm:ss z"
                              timeZone=timezone
                            }}</span>
                        </div>
                      {{/if}}
                      {{#if report.type}}
                        <div>
                          <span class="p-key">Type:</span>
                          <span class="p-value">{{report.type}}</span>
                        </div>
                      {{/if}}
                      {{#if report.report_id}}
                        <div
                          title="NOTE: Some Reports will lead to 404 pages. This is expected."
                          class="p-link"
                        >
                          <a
                            href="https://advantage.mandiant.com/reports/{{report.report_id}}"
                          >
                            View Report in Mandiant
                            {{fa-icon
                              icon="external-link-square"
                              class="external-link-icon"
                            }}
                          </a>
                        </div>
                      {{/if}}
                    </div>
                  {{/each}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "indicatorReports" 0)}}
                >
                  <span class="text">Reports</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            </div>
          {{/if}}

          {{#if indicatorV4.campaigns}}
            <div class="indicator-campaigns">
              {{#if (get expandableTitleStates (concat "indicatorCampaigns" 0))}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "indicatorCampaigns" 0)}}
                >
                  <span class="text">Campaigns</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 indicator-campaigns-content">
                  {{#each indicatorV4.campaigns as |campaign|}}
                    <div class="mb-2 indicator-report">
                      {{#if campaign.title}}
                        <div>
                          <span class="p-key">Title:</span>
                          <span class="p-value">{{campaign.title}}</span>
                        </div>
                      {{/if}}
                      {{#if campaign.name}}
                        <div>
                          <span class="p-key">Name:</span>
                          <span class="p-value">{{campaign.name}}</span>
                        </div>
                      {{/if}}
                      {{#if campaign.id}}
                        <div class="p-link">
                          <a
                            href="https://advantage.mandiant.com/campaigns/{{campaign.id}}"
                          >
                            View Campaign in Mandiant
                            {{fa-icon
                              icon="external-link-square"
                              class="external-link-icon"
                            }}
                          </a>
                        </div>
                      {{/if}}
                    </div>
                  {{/each}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "indicatorCampaigns" 0)}}
                >
                  <span class="text">Campaigns</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            </div>
          {{/if}}

          {{#if indicatorV4.associated_hashes}}
            <div class="associated-hashes">
              {{#if (get expandableTitleStates (concat "associateHashes" 0))}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "associateHashes" 0)}}
                >
                  <span class="text">Associated Hashes</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 associated-hashes-content">
                  {{#each indicatorV4.associated_hashes as |associated_hash|}}
                    <div class="associated-hash">
                      <div>
                        <span class="p-key">Type:</span>
                        <span class="p-value">{{associated_hash.type}}</span>
                      </div>
                      <div>
                        <span class="p-key">Value:</span>
                        <span class="p-value">{{associated_hash.value}}</span>
                      </div>
                    </div>
                  {{/each}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "associateHashes" 0)}}
                >
                  <span class="text">Associated Hashes</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            </div>
          {{/if}}

          {{#if mispV4}}
            <div class="misp-v4">
              {{#if (get expandableTitleStates (concat "mispV4" 0))}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "mispV4" 0)}}
                >
                  <span class="text">MISP</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 misp-v4-content">
                  {{#each mispV4 as |mispV4Value|}}
                    <div>
                      <span class="p-key">{{titleize
                          (humanize (dasherize (get mispV4Value 0)))
                        }}:</span>
                      <span class="p-value">True</span>
                    </div>
                  {{/each}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on "click" (action "toggleExpandableTitle" "mispV4" 0)}}
                >
                  <span class="text">MISP</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            </div>
          {{/if}}
        {{/with}}
      </div>
    {{/if}}

    {{#if (eq activeTab "vulnerabilities")}}
      <div class="vulnerabilities-results">
        {{#each details.vulnerabilities as |vulnerability vulnerabilityIndex|}}
          <div class="mb-2 vulnerabilities-result">
            <h1 class="p-title">
              {{fa-icon icon="fingerprint"}}
              {{vulnerability.cve_id}}
            </h1>
            <div class="p-link">
              <a
                href="https://advantage.mandiant.com/vulnerabilities/{{vulnerability.id}}"
              >
                View in Mandiant
                {{fa-icon icon="external-link-square" class="external-link-icon"}}
              </a>
            </div>
            {{#if vulnerability.mve_id}}
              <div>
                <span class="p-key">MVE ID:</span>
                <span class="p-value">{{vulnerability.mve_id}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.cwe_details.id}}
              <div>
                <span class="p-key">CWE ID:</span>
                <span class="p-value">{{vulnerability.cwe_details.id}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.cwe_details.title}}
              <div>
                <span class="p-key">CWE Title:</span>
                <span class="p-value">{{vulnerability.cwe_details.title}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.risk_rating}}
              <div>
                <span class="p-key">Risk Rating:</span>
                <span class="p-value">{{vulnerability.risk_rating}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.days_to_patch}}
              <div>
                <span class="p-key">Days to Patch:</span>
                <span class="p-value">{{vulnerability.days_to_patch}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.exploitation_state}}
              <div>
                <span class="p-key">Exploitation State:</span>
                <span class="p-value">{{vulnerability.exploitation_state}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.exploitation_consequence}}
              <div>
                <span class="p-key">Exploitation Consequence:</span>
                <span class="p-value">{{vulnerability.exploitation_consequence}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.audience.length}}
              <div>
                <span class="p-key">Audiences:</span>
                <span class="p-value">
                  {{#each vulnerability.audience as |audience index|}}
                    {{titleize (humanize (dasherize (or audience.name audience)))}}
                    {{#if audience.license}}({{titleize
                        (humanize (dasherize audience.license))
                      }}){{/if}}{{#if
                      (not (eq (sub vulnerability.audience.length 1) index))
                    }}, {{/if}}
                  {{/each}}
                </span>
              </div>
            {{/if}}
            {{#if vulnerability.aliases.length}}
              <div>
                <span class="p-key">Aliases:</span>
                <span class="p-value">{{join ", " vulnerability.aliases}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.available_mitigation.length}}
              <div>
                <span class="p-key">Available Mitigation:</span>
                <span class="p-value">{{join
                    ", "
                    vulnerability.available_mitigation
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.epss.percentile}}
              <div>
                <span class="p-key">EPSS Percentile:</span>
                <span class="p-value">{{vulnerability.epss.percentile}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.epss.score}}
              <div>
                <span class="p-key">EPSS Score:</span>
                <span class="p-value">{{vulnerability.epss.score}}</span>
              </div>
            {{/if}}

            {{#if vulnerability.first_publish_date}}
              <div>
                <span class="p-key">First Publish Date:</span>
                <span class="p-value">{{moment-format
                    vulnerability.first_publish_date
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.publish_date}}
              <div>
                <span class="p-key">Publish Date:</span>
                <span class="p-value">{{moment-format
                    vulnerability.publish_date
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.last_modified_date}}
              <div>
                <span class="p-key">Last Modified Date:</span>
                <span class="p-value">{{moment-format
                    vulnerability.last_modified_date
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.date_of_disclosure}}
              <div>
                <span class="p-key">Date of Disclosure:</span>
                <span class="p-value">{{moment-format
                    vulnerability.date_of_disclosure
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.cisa_known_exploited.added_date}}
              <div>
                <span class="p-key">CISA Known Exploited Date Added:</span>
                <span class="p-value">{{moment-format
                    vulnerability.cisa_known_exploited.added_date
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}
            {{#if vulnerability.cisa_known_exploited.due_date}}
              <div>
                <span class="p-key">CISA Known Exploited Due Date:</span>
                <span class="p-value">{{moment-format
                    vulnerability.cisa_known_exploited.due_date
                    "MM/DD/YYYY HH:mm:ss z"
                    timeZone=timezone
                  }}</span>
              </div>
            {{/if}}

            {{#if (or vulnerability.affects_ot (eq vulnerability.affects_ot false))}}
              <div>
                <span class="p-key">Affects OT:</span>
                <span class="p-value">{{vulnerability.affects_ot}}</span>
              </div>
            {{/if}}
            {{#if
              (or
                vulnerability.observed_in_the_wild
                (eq vulnerability.observed_in_the_wild false)
              )
            }}
              <div>
                <span class="p-key">Observed in the Wild:</span>
                <span class="p-value">{{vulnerability.observed_in_the_wild}}</span>
              </div>
            {{/if}}
            {{#if (or vulnerability.was_zero_day (eq vulnerability.was_zero_day false))}}
              <div>
                <span class="p-key">Was Zero Day:</span>
                <span class="p-value">{{vulnerability.was_zero_day}}</span>
              </div>
            {{/if}}
            {{#if (or vulnerability.intel_free (eq vulnerability.intel_free false))}}
              <div>
                <span class="p-key">Intel Free:</span>
                <span class="p-value">{{vulnerability.intel_free}}</span>
              </div>
            {{/if}}
            {{#if
              (or vulnerability.is_publishable (eq vulnerability.is_publishable false))
            }}
              <div>
                <span class="p-key">Is Publishable:</span>
                <span class="p-value">{{vulnerability.is_publishable}}</span>
              </div>
            {{/if}}
            {{#if (or vulnerability.is_predicted (eq vulnerability.is_predicted false))}}
              <div>
                <span class="p-key">Is Predicted:</span>
                <span class="p-value">{{vulnerability.is_predicted}}</span>
              </div>
            {{/if}}
            {{#if (or vulnerability.is_predicted (eq vulnerability.is_predicted false))}}
              <div>
                <span class="p-key">Verdict Rating Source:</span>
                <span class="p-value">
                  {{#if (not vulnerability.risk_rating)}}
                    Unrated
                  {{else}}
                    {{#if vulnerability.is_predicted}}
                      Machine Learning
                    {{else}}
                      Analyst
                    {{/if}}
                  {{/if}}
                </span>
              </div>
            {{/if}}
            {{#if vulnerability.predictionScoresBase}}
              <div>
                <span class="p-key">Predictive Score Base (CVSS):</span>
                <span class="p-value">{{vulnerability.predictionScoresBase}}</span>
              </div>
            {{/if}}
            {{#if vulnerability.predictionScoresTemporal}}
              <div>
                <span class="p-key">Predictive Score Temporal (CVSS):</span>
                <span class="p-value">{{vulnerability.predictionScoresTemporal}}</span>
              </div>
            {{/if}}

            {{#if vulnerability.description}}
              {{#if
                (get
                  expandableTitleStates
                  (concat "vulnerabilityDescription" vulnerabilityIndex)
                )
              }}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityDescription"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Description</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 vulnerability-description">
                  {{html-safe vulnerability.description}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityDescription"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Description</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            {{/if}}

            {{#if vulnerability.analysis}}
              {{#if
                (get
                  expandableTitleStates
                  (concat "vulnerabilityAnalysis" vulnerabilityIndex)
                )
              }}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle" "vulnerabilityAnalysis" vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Analysis</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 vulnerability-analysis">
                  {{html-safe vulnerability.analysis}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle" "vulnerabilityAnalysis" vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Analysis</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            {{/if}}

            {{#if vulnerability.executive_summary}}
              {{#if
                (get
                  expandableTitleStates
                  (concat "vulnerabilityExecutiveSummary" vulnerabilityIndex)
                )
              }}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityExecutiveSummary"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Executive Summary</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 vulnerability-executive-summary">
                  {{html-safe vulnerability.executive_summary}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityExecutiveSummary"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Executive Summary</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            {{/if}}

            {{#if vulnerability.vulnerable_products}}
              {{#if
                (get
                  expandableTitleStates
                  (concat "vulnerabilityVulnerableProducts" vulnerabilityIndex)
                )
              }}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityVulnerableProducts"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Vulnerable Products</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 vulnerability-vulnerable-products">
                  {{html-safe vulnerability.vulnerable_products}}
                </div>
              {{else}}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityVulnerableProducts"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Vulnerable Products</span>
                  {{fa-icon "angle-down"}}
                </a>
              {{/if}}
            {{/if}}

            {{#if (or vulnerability.workarounds vulnerability.workarounds_list.length)}}
              {{#if
                (get
                  expandableTitleStates
                  (concat "vulnerabilityWorkarounds" vulnerabilityIndex)
                )
              }}
                <a
                  class="expandable-title"
                  {{on
                    "click"
                    (action
                      "toggleExpandableTitle"
                      "vulnerabilityWorkarounds"
                      vulnerabilityIndex
                    )
                  }}
                >
                  <span class="text">Workarounds</span>
                  {{fa-icon "angle-up"}}
                </a>
                <div class="ml-2 vulnerability-workarounds">
                  {{#if vulnerability.workarounds_list.length}}
                    {{#each vulnerability.workarounds_list as |workaround|}}
